
	PROC LDRAGON ( LEVEL )
	    IF LEVEL THEN
		FORWARD 5 
	    ELSE
		LDRAGON ( LEVEL )
		LEFT 90
		RDRAGON ( LEVEL )
	    ENDIF 
	    

	PROC RDRAGON ( LEVEL )
	    IF LEVEL THEN
		FORWARD 5 
	    ELSE
		LDRAGON ( LEVEL )
		RIGHT 90
		RDRAGON ( LEVEL )
	    ENDIF 

	PROC MAIN (VOID)
	   LDRAGON ( 11 )
